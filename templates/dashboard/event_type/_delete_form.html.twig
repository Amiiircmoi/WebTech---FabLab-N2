<form method="post" action="{{ path('app_dashboard_type_delete', {'id': type.id}) }}" onsubmit="return confirm('Êtes vous sûr de vouloir supprimer ce type ?');">
	<input type="hidden" name="_token" value="{{ csrf_token('delete' ~ type.id) }}">
	<button title="Supprimer" class="btn btn-circle btn-danger">
		<i class="fa-solid fa-trash"></i>
	</button>
</form>
